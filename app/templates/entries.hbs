<!-- /journals/:year/entries -->
<div class="row">
  <div class="row list-wrapper">
    <ul class="list-group entries">
      {{#each entry in model}}
    <li class="list-group-item">{{entry.date}}
      <span class="location pull-right">{{entry.location.name}}</span>
    </li>
    {{/each}}
  </ul>
</div>
</div>
