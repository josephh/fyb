<!-- entries.hbs -->
<div class="row">
  <ol class="list-group entries">
    {{#each model as |entry|}}
      <li class="list-group-item">
        <strong>date:</strong>
        <em>{{entry.date}}</em>
        <strong>(id: {{entry.id}})</strong>
        <strong>(journal id: {{entry.journalId}})</strong>
        <ul class="list-group fishes">
          {{#each entry.fishes as |fish|}}
            <li class="list-group-item">
              <span class="fishes">{{fish.species}}</span>
            </li>
          {{/each}}
        </ul>
      </li>
    {{/each}}
  </ol>
</div>
